        <UserSecretsId>4bc92a8a-1bef-40be-8a28-bd1387889571</UserSecretsId>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Blazorise.Bootstrap5" Version="1.7.6" />
